<?xml version="1.0" encoding="UTF-8"?>
<setup xmlns="https://www.benerator.de/schema/3.0.0"
>
    <import domains="person"/>
    <import platforms="csv, multi"/>
    <generate type="greeting" count="35" consumer="MultiExporter">
        <variable name="person" dataset="DE" generator="new PersonGenerator{minAgeYears='16', maxAgeYears='122'}"
                  locale="de"/>
        <variable name="vorname" script="person.givenName"/>
        <variable name="nachname" script="person.familyName"/>
        <variable name="geschlecht" map="'MALE'->
            'm','FEMALE'->'w'" script="person.gender.name()"/>
        <attribute name="salutation" values="'Hello','Hi','Howdy','How it is going','How was your day '"/>
        <attribute name="name" script="vorname"/>
        <attribute name="surname" script="nachname"/>
        <attribute name="gender" script="geschlecht"/>
        <attribute name="birthdate" type="date" script="person.birthDate"/>
        <attribute name="salutation" values="'Hello','Hi','Howdy','How it is going','How was your day '"/>
        <attribute name="name2" script="vorname"/>
        <attribute name="surname2" script="nachname"/>
        <attribute name="gender2" script="geschlecht"/>
        <attribute name="birthdate2" type="date" script="person.birthDate"/>
        <attribute name="email" script="person.email"/>
    </generate>
