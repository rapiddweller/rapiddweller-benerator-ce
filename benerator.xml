<setup xmlns="https://www.benerator.de/schema/3.0.0">
    <bean id="zdtGen" class="ZonedDateTimeGenerator">
        <property name='minDate' value='2020-01-02'/>
        <property name='maxDate' value='2020-11-30'/>
        <property name='dateGranularity' value='0-0-2'/>
        <property name='minTime' value='09:00'/>
        <property name='maxTime' value='12:00'/>
        <property name='timeGranularity' value='00:05:00'/>
        <property name='zone' value='America/Chicago'/>
    </bean>

    <memstore id="mem"/>

    <generate type="x" count="20" consumer="mem">
        <attribute name="y" generator="zdtGen"/>
    </generate>
</setup>